# Supabase Configuration
SUPABASE_URL=http://localhost:54321
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Server Configuration  
PORT=8080
NODE_ENV=development

# Authentication for Cloud Run endpoints
CLOUD_RUN_AUTH_TOKEN=your-secure-auth-token

# Vault Configuration (used for direct secret access)
VAULT_SECRET_KEY=your-vault-encryption-key

# Logging Configuration
LOG_LEVEL=info

# Processing Configuration
MAX_TURNS_PER_ANALYSIS=50
ANALYSIS_TIMEOUT_MS=300000

# Cost Management
ESTIMATED_COST_PER_TOKEN=0.000003