{
  "name": "hackscore-cloudrun-worker",
  "version": "1.0.0",
  "description": "ClaudeCode worker for HackScoreAI on Cloud Run",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "echo 'No build step required'",
    "test": "echo 'No tests specified'",
    "dev:setup": "./scripts/dev.sh",
    "deploy": "./scripts/deploy.sh",
    "docker:build": "docker build -t hackscore-claudecode-worker .",
    "docker:run": "docker run -p 8080:8080 --env-file .env hackscore-claudecode-worker"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.31",
    "@supabase/supabase-js": "^2.39.3",
    "express": "^4.18.2",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}